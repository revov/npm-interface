{
  "name": "npm-interface",
  "version": "0.0.1",
  "description": "A cross-platform user interface for NPM",
  "main": "main.js",
  "scripts": {
    "build": "ngc -p browser && node ./scripts/bundle.js",
    "start": "cross-env ELECTRON_ENV=development concurrently --kill-others \"tsc --pretty -p browser --watch\" \"electron .\"",
    "start:prod": "cross-env ELECTRON_ENV=production electron ."
  },
  "keywords": [
    "npm",
    "graphical",
    "user",
    "interface"
  ],
  "author": "Stoyan Revov",
  "license": "MIT",
  "dependencies": {
    "cross-env": "2.0.0",
    "electron-prebuilt": "1.3.3"
  },
  "devDependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/compiler-cli": "0.5.0",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/platform-server": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.2",
    "bootstrap": "4.0.0-alpha.3",
    "concurrently": "2.2.0",
    "marked": "0.3.6",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.36",
    "systemjs-builder": "0.15.26",
    "systemjs-plugin-css": "0.1.26",
    "typescript": "1.9.0-dev.20160627-1.0",
    "zone.js": "0.6.13"
  }
}
